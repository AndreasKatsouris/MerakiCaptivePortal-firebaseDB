{
  "type": "module",
  "name": "meraki-captive-portal",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "verify": "node scripts/verify-setup.js",
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "build-css-v2": "tailwindcss -c tailwind-v2.config.js -i PUBLIC/css/dashboard-v2.css -o PUBLIC/css/dashboard-v2-compiled.css",
    "watch-css-v2": "tailwindcss -c tailwind-v2.config.js -i PUBLIC/css/dashboard-v2.css -o PUBLIC/css/dashboard-v2-compiled.css --watch",
    "dev-v2": "npm run build-css-v2 && vite dev",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,vue,css,md,json}\"",
    "format:check": "prettier --check \"**/*.{js,vue,css,md,json}\""
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.0.0",
    "@google-cloud/vision": "^4.3.2",
    "@types/google.maps": "^3.54.10",
    "axios": "^1.7.8",
    "bootstrap": "^5.3.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.7",
    "express": "^4.21.1",
    "firebase": "^11.2.0",
    "firebase-admin": "^12.7.0",
    "firebase-functions": "^6.1.1",
    "lodash": "^4.17.21",
    "lucide-react": "^0.488.0",
    "pinia": "^2.3.1",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "twilio": "^5.3.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@tailwindcss/forms": "^0.5.10",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^10.6.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "tailwindcss": "^4.1.11",
    "vite": "^6.0.0"
  }
}
